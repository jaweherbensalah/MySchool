<!-- search section -->
<section class="search-section ss-other-page">
    <div class="container">
        <div class="search-warp">
            <div class="section-title text-white">
                <h2><span>Search your course</span></h2>
            </div>
            <div class="row">
                <div class="col-lg-10 offset-lg-1">
                    <!-- search form -->
                    <form class="course-search-form">
                        <input type="text" placeholder="Course">
                        <input type="text" class="last-m" placeholder="Category">
                        <button class="site-btn btn-dark">Recherche Events</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- search section end -->


<!-- course section -->

<section class="course-section spad pb-0">
    <div class="course-warp">
        <div class="row course-items-area">
            {% for f in formations %}
            <!-- course -->
            <div class="mix col-lg-3 col-md-4 col-sm-6 finance">
                <div class="course-item">
                    <div class="course-thumb set-bg" data-setbg="{{ asset('img/' ~ f.image) }}"  height="100px" width="250px">
                        <div class="price">{{f.dateevent|date }}</div>
                    </div>
                    <div class="course-info">
                        <div class="course-text">
                            <h5>{{f.nomEvent }}</h5>
                            <p>{{ f.description }}</p>
                            <div class="students">{{ f.placeDispo }}</div>
                        </div>
                        <div class="course-author">
                            <div class="ca-pic set-bg" data-setbg="img/authors/1.jpg"></div>
                            <p>{{ f.heureevent }} </p>
                        </div>
                    </div>
                </div>
            </div>
            {% endfor %}
            <!-- course -->



