{% extends 'default/index.html.twig' %}
        {% block  changer  %}
            <br>
            <br>
            <br>
            <br>

            <div class="col-md-12">
                <div class="card">
                    <div class="card-header card-header-primary">
                        <h4 class="card-title ">liste des élèves abonnés au Transport !</h4>
                        {#                        <p class="card-category"> Here is a subtitle for this table</p>#}
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table">
                                <thead class=" text-primary">
                                <th>  Email</th>
                                <th>  Nom Utilisateur</th>
                                <th>Solde</th>
                                <th>Mot De Passe</th>
                                <th >Durée De L'abonnement</th>

                                <tbody>
                                {% for f in personnes %}


                                    <tr>
                                        <td> {{ f.email }} </td>
                                        <td> {{ f.username }} </td>
                                        <td> {{ f.solde }} </td>
                                        <td> {{ f.password }} </td>
                                        <td> {{ f.dureeAbonnement }} </td>

                                    </tr>

                                {% endfor %}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>


            <div><a href="{{ path('imprimeAbonneTransport') }}"><span class="glyphicon glyphicon-menu-left" aria-hidden="true"></span>Imprimer cette liste ?</a></div>
        {% endblock %}

