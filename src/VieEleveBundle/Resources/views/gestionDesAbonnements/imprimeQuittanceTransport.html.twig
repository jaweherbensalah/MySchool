<b>Tunis le{{ "now"|date("m/d/Y") }}</b>
<div><img width="200px;" src="menu/logo.png.png" alt="MyShcool"> </div>

<br><br><br>


<div class="col-md-12">
    <div class="card">
        <div class="card-header card-header-primary">
            <h2 class="card-title " style="color:#8959A8;">Facture de l'élève numéro : {{ eleve.id }} Nommé : {{ eleve.username }}</h2>
            {#                        <p class="card-category"> Here is a subtitle for this table</p>#}
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table" border="1" width="500" >
                    <thead class=" text-primary">
                    <tr> <th>  Date Inscription</th>
                        <th>  Fin Abonnement</th>
                        <th>Montant Payé</th>

                    </tr>
                    <tbody>
                    {% for s in quittance %}


                        <tr>
                            <td> {{ s.debutInscription | date }} </td>
                            <td>{{ s.finInscription | date }}  </td>
                            <td> {{ s.prix }}    </td>



                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
